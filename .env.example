# Server Configuration
PORT=8080
ENV=development

# API Keys (comma-separated)
API_KEYS=demo-key-123,fusio-gateway-key,test-key-456

# Rate Limiting (requests per minute)
RATE_LIMIT=100

# Dremio Configuration (for Iceberg queries)
DREMIO_HOST=your-dremio-host.com
DREMIO_PORT=31010
DREMIO_USERNAME=your-username
DREMIO_PASSWORD=your-password
# Or use token instead of username/password
# DREMIO_TOKEN=your-dremio-token

# BigQuery Configuration
BIGQUERY_PROJECT_ID=your-gcp-project-id
BIGQUERY_DATASET_ID=your-dataset-id
# Path to service account JSON
GOOGLE_APPLICATION_CREDENTIALS=./credentials/bigquery-key.json

# Redis Configuration (for caching)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0